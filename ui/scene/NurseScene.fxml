<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="managerPanel" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="768.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.NurseController">
    <top>
        <HBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" spacing="10">
            <Region HBox.hgrow="ALWAYS"/>
            <MenuButton alignment="TOP_LEFT" mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0">
                <items>
                    <MenuItem fx:id="profileMenuItem" mnemonicParsing="false" onAction="#showProfileDialog"
                              text="Profile"/>
                    <MenuItem fx:id="logoutMenuItem" mnemonicParsing="false" onAction="#logout" text="Log out"/>
                </items>
                <graphic>
                    <AnchorPane prefHeight="25.0" prefWidth="25.0"/>
                </graphic>
            </MenuButton>
        </HBox>
    </top>
    <center>
       <TableView fx:id="nursePatients" prefHeight="200.0" prefWidth="200.0">
          <columnResizePolicy>
             <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
             <TableColumn prefWidth="75.0" text="ID"/>
             <TableColumn prefWidth="75.0" text="Priority"/>
             <TableColumn prefWidth="75.0" text="Name" />
             <TableColumn prefWidth="75.0" text="Last Name" />
             <TableColumn prefWidth="75.0" text="Complaint" />
             <TableColumn prefWidth="30.0" text="Gender" />
             <TableColumn prefWidth="30.0" text="Age" />
             <TableColumn prefWidth="30.0" text="Blood Type"/>
             <TableColumn prefWidth="75.0" text="Appointment"/>
          </columns>
          <columnResizePolicy>
             <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
       </TableView>
    </center>
</BorderPane>
